{
  "name": "Application.Web.Express",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@types/express": "^4.17.2",
    "@types/react": "^16.9.19",
    "@types/webpack-env": "^1.15.1",
    "@vue/compiler-sfc": "^3.0.0-alpha.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "express": "^4.17.1",
    "hypernova": "^2.5.0",
    "hypernova-react": "^2.1.0",
    "isomorphic-style-loader": "^5.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "vue": "^2.6.11",
    "vue-loader": "15.8.3",
    "vue-router": "^3.1.5",
    "vue-server-renderer": "^2.6.11",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "build:both": "concurrently \"yarn build:server\" \"yarn build:client\" --names build:server,build:client",
    "build:server": "webpack --config build/webpack.server.js",
    "build:client": "webpack --config build/webpack.client.js",
    "start:server": "node ./dist/server/index.js",
    "start:watch": "concurrently \"yarn build:both\" \"nodemon\" --names build:both,start:server"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "dist/server/*"
    ],
    "exec": "yarn start:server",
    "ext": "js"
  }
}
